<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.quickbird.speedtest"
    android:versionCode="2891"
    android:versionName="1.5.0" >

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.CHANGE_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="com.android.launcher.permission.READ_SETTINGS" />
    <uses-permission android:name="android.permission.CHANGE_WIFI_STATE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_LOGS" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.CHANGE_CONFIGURATION" />

    <supports-screens
        android:anyDensity="true"
        android:largeScreens="true"
        android:normalScreens="true"
        android:smallScreens="true" >
    </supports-screens>

    <uses-sdk
        android:maxSdkVersion="17"
        android:minSdkVersion="7"
        android:targetSdkVersion="7" />

    <application
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <meta-data
            android:name="com.amap.api.v2.apikey"
            android:value="f1e4d9a7e67a8768e4b9cba8383a0d09" />

        <activity
            android:name=".gui.activity.MainActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".gui.activity.TestSpeedActivity"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name=".gui.activity.SpeedHistoryActivity"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name=".gui.activity.MoreActivity"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name=".gui.activity.ShareActivity"
            android:configChanges="orientation|keyboardHidden"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="adjustUnspecified|stateHidden" >
        </activity>
        <activity
            android:name=".gui.activity.TecentShareActivity"
            android:configChanges="orientation|keyboardHidden"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="adjustUnspecified|stateHidden" >
        </activity>
        <activity
            android:name=".gui.activity.ShowLocationActivity"
            android:configChanges="orientation|keyboardHidden"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="adjustUnspecified|stateHidden" >
        </activity>
        <activity
            android:name=".gui.activity.AboutActivity"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name=".gui.activity.UploadActivity"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name=".gui.activity.SpeedTestActivity"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name=".gui.activity.SpeedResultActivity"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name=".gui.activity.AccountManageActivity"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name=".gui.activity.NetWorkActivity"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name=".gui.activity.SpeedValueDetailActivity"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name=".gui.activity.LoadingActivity"
            android:screenOrientation="portrait" >
        </activity>

        <!-- OAuth Version 1.0 使用 WebView 辅助授权必须 -->
        <activity
            android:name="com.tencent.weibo.webview.OAuthV1AuthorizeWebView"
            android:label="@string/app_name"
            android:screenOrientation="portrait" >
        </activity>

        <!-- OAuth Version 2. 使用  WebView 辅助进行ImplicitGrant方式授权必须 -->
        <activity
            android:name="com.tencent.weibo.webview.OAuthV2AuthorizeWebView"
            android:label="@string/app_name"
            android:screenOrientation="portrait" >
        </activity>
        
        <service
            android:name="com.umeng.common.net.DownloadingService"
            android:exported="true"
            android:process=":DownloadingService" >
        </service>
        
        <activity
            android:name="com.umeng.fb.ui.SendFeedback"
            android:windowSoftInputMode="adjustResize" />
        <activity android:name="com.umeng.fb.ui.FeedbackConversations" />
        <activity
            android:name="com.umeng.fb.ui.FeedbackConversation"
            android:configChanges="keyboard" />

        <meta-data
            android:name="UMENG_APPKEY"
            android:value="512f23195270155cf4000037" >
        </meta-data>
        <!-- channel :values is your channel name 4eb7a8135270152e22000015 -->

        <meta-data
            android:name="UMENG_CHANNEL"
            android:value="xiaozhan" />
    </application>

</manifest>